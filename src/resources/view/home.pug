extends main 
block content
    .container
        .top.slide
            button(class='btn-arrow-left')
                i(class='fas fa-arrow-circle-left icon_move')
            div(class='slide_box')
                img(class='slide_img-' src='data:image/jpeg;base64,' alt='')
            div
                h1(class='slide_title text') Mục Tiêu Khóa Học
                p(class='slide_text text') Giúp Cách Học Viên Học Tập Kiến Thức Mới Và Môi Trường Học Tập Hiệu Quả
            button 
                i(class='fas fa-chevron-circle-right icon_move')
    div
        .container
            .main
                
                .row.card-2
                    each Coures in coures
                        .col-lg-4.col-sm-6.card-layout.about
                            //- .card.card-size
                            //-     a(href='#')
                            //-         img.card-img-top.img-size(src=`${Coures.img}` alt='Card image cap')
                            //-     .car
                            //-     d-body
                            //-         a(href='#')
                            //-             h5.card-title=Coures.name
                            //-         p.card-text=Coures.mota   
                            //-         a.btn.btn-primary(href=`/Coures/${Coures.slug}`)  Learn Now
                            .card(style="width: 18rem;")
                                img.card-img-top(src=`${Coures.img}`, alt="javascript")
                                .card-body
                                    h5.card-title=Coures.name
                                    p.card-text=Coures.mota
                                    a.btn.btn-primary(href=`/Coures/${Coures.slug}`) learn now